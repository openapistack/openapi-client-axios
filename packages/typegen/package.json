{
  "name": "openapi-client-axios-typegen",
  "description": "Typescript type generator for openapi-client-axios",
  "version": "7.8.0",
  "author": "Viljami Kuosmanen <viljami@viljami.io>",
  "funding": "https://github.com/sponsors/anttiviljami",
  "homepage": "https://openapistack.co",
  "license": "MIT",
  "bin": {
    "typegen": "./typegen"
  },
  "files": [
    "typegen",
    "*.js",
    "*.d.ts",
    "**/*.js",
    "**/*.d.ts",
    "!*.jest.config.js",
    "!*.png",
    "!src/**",
    "!__tests__/**",
    "!*.config.js"
  ],
  "keywords": [
    "openapi",
    "swagger",
    "client",
    "axios",
    "typegen",
    "frontend",
    "browser",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openapistack/openapi-client-axios.git"
  },
  "bugs": {
    "url": "https://github.com/openapistack/openapi-client-axios/issues"
  },
  "main": "typegen.js",
  "types": "typegen.d.ts",
  "dependencies": {
    "@anttiviljami/dtsgenerator": "^3.20.0",
    "@apidevtools/json-schema-ref-parser": "^11.2.0",
    "axios": ">=0.25.0",
    "indent-string": "^4.0.0",
    "lodash": "^4.17.21",
    "openapi-client-axios": "^7.8.0",
    "openapi-types": "^12.1.0",
    "yargs": "^17.3.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.178",
    "@types/yargs": "^17.0.7",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "build": "tsc",
    "watch-build": "tsc -w",
    "prepare": "npm run build",
    "test": "NODE_ENV=test jest",
    "lint": "prettier --check src/**/*.ts",
    "lint:fix": "prettier --write src/**/*.ts"
  },
  "gitHead": "6ea364b653a2264ddd2de4d2f1ddabc5cf3cbfd5"
}
