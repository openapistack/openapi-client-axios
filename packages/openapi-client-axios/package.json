    "axios": "^1.6.2",
    "axios-mock-adapter": "^1.22.0",
    "jest": "^29.7.0",
    "json-schema": "^0.4.0",
    "json-schema-deref-sync": "^0.14.0",
    "msw": "^1.3.2",
    "prettier": "^3.0.3",
